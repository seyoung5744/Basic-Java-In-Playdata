package day02;

public class ifExam01 {

	public static void main(String[] args) {
		int i = 10;
		int j = 20;

		if (i == j) {
			System.out.println("i와 j는 같아요.");
			System.out.println("i==j");
		}else if(i>j){
			System.out.println("i가 j보다 커요.");
			System.out.println("i>j");
		}else {
			System.out.println("몰라요~~ ");
		}

	}

}
