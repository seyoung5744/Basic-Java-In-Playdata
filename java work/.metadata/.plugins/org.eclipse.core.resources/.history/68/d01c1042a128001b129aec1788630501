package day02;

public class ArrayExam03 {
	public static void main(String[] args) {
		int [][][][] arr;
		int [][] arr1[][];
		int [] arr2[];
		int [][] arr3;
		int arr4[][];
		
		int [][] arr5 = new int[3][2];
		arr5[0][1] = 10;
		
		System.out.println(arr5[0][1]);
		
		int[][] iarr5 = new int[3][]; // [] 아무것도 없으면 null
//		iarr5[0][1] = 10; // 오류
		
		String str = null;
		str.charAt(0)
	}
}
