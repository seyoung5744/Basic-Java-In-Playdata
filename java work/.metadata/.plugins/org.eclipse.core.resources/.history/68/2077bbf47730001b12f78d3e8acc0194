package day09;

import java.io.IOException;

public class IOExam03 {
	public static void main(String[] args) {
		int bt;
		try {
			while(bt = System.in.read() != -1) {
				System.out.println((char)bt);				
			}
		} catch (Exception e) {
			// TODO: handle exception
			e.printStackTrace();
		}
	}
}
