package day09;

import java.io.IOException;

public class IOExam03 {
	public static void main(String[] args) {
		int bt;
		int count = 0;
		try {
			//-1은 아스키에 없어서 -1이 넘어오면 끝
			while((bt = System.in.read()) != -1) { 
				System.out.println((char)bt);		
				count++;
			}
		} catch (Exception e) {
			// TODO: handle exception
			e.printStackTrace();
		}
	}
}
