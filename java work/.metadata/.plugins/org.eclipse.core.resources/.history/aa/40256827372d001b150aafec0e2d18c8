package day05;

class Parend4{
	int i = 5;
	public int get() {
		return i;
	}
//	public void print() {
//		System.out.println(i);
//	}
}

class Child extends Parend4{
	int i = 10;
	public int get() {
		return i;
	}
	
	public void print() {
		System.out.println(i);
	}
}


public class Test4 {
	public static void main(String[] args) {
		Parend4 c = new Child();
		System.out.println(c.i);
		System.out.println(c.get());
		((Child)c).print();
	}
}
