package day04;

public class Car {
	private String name;
	private int speed;
	static int numberOfCars;
	
	public Car() {
		this.name = "MyCar";
		this.speed = 0;
		numberOfCars++;
	}
	
	public void setName(String name) { this.name = name; }
	
	public void setSpeed(int speed) { this.speed = speed; }
	
	public int getSpeed() { return this.speed; }
}
